{"ast":null,"code":"import { Swiper, SwiperSlide } from 'swiper/vue'; // Import Swiper styles\n\nimport 'swiper/css';\nimport 'swiper/css/effect-cards'; // import Swiper core and required modules\n\nimport SwiperCore, { EffectCards } from 'swiper'; // install Swiper modules\n\nSwiperCore.use([EffectCards]);\nimport { api } from '@/config/site.config';\nimport json from '../recipes.json';\nexport default {\n  name: 'HomePage',\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n\n  data() {\n    return {\n      mensaje: 'Hola!',\n      userId: 24,\n      idCategory1: 37,\n      idCategory2: 38,\n      products: [],\n      productsRoutes: [],\n      recipes: json.recipes\n    };\n  },\n\n  methods: {\n    cargarProductos(id) {\n      api.get('categories/' + this.userId + '/products/' + id).then(response => {\n        this.products = response.data.products;\n      });\n    },\n\n    scrollToTop() {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      });\n    }\n\n  },\n  computed: {\n    loggedIn() {\n      return this.$store.state.auth.status.loggedIn;\n    }\n\n  },\n  watch: {},\n\n  created() {},\n\n  mounted() {\n    this.cargarProductos(this.idCategory1);\n    api.get('categories/' + this.userId + '/products/' + this.id).then(response => {\n      for (let x in response.data.products) {\n        this.productsRoutes.push({\n          route: '/product/' + response.data.products[x].id\n        });\n      }\n    });\n  }\n\n};","map":{"version":3,"sources":["C:\\Users\\JELOSKA ISABEL\\Documents\\wild-souls\\Wild-Souls\\src\\pages\\Home.vue"],"names":[],"mappings":"AAoUA,SAAS,MAAT,EAAiB,WAAjB,QAAoC,YAApC,C,CAEA;;AACA,OAAO,YAAP;AAEA,OAAO,yBAAP,C,CAEA;;AACA,OAAO,UAAP,IAAqB,WAArB,QAAwC,QAAxC,C,CAEA;;AACA,UAAU,CAAC,GAAX,CAAe,CAAC,WAAD,CAAf;AAEA,SAAS,GAAT,QAAoB,sBAApB;AACA,OAAO,IAAP,MAAiB,iBAAjB;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,UADO;AAEb,EAAA,UAAU,EAAE;AACV,IAAA,MADU;AAEV,IAAA;AAFU,GAFC;;AAMb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,OAAO,EAAE,OADJ;AAEL,MAA