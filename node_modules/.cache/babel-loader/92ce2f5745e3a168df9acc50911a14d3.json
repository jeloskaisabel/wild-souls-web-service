{"ast":null,"code":"import { api } from '@/config/site.config';\nexport default {\n  name: 'CategoriaPage',\n  props: ['id'],\n\n  data() {\n    return {\n      products: [],\n      categoryName: ''\n    };\n  },\n\n  methods: {\n    cargarProductos(id) {\n      api.get('categories/products/' + id).then(response => {\n        this.products = response.data.products;\n      });\n    },\n\n    findCategory(id) {\n      api.get('categories/getAll').then(response => {\n        for (let category of response.data.categories) {\n          if (category.id == id) {\n            this.categoryName = category.category_name;\n          }\n        }\n      });\n    }\n\n  },\n  watch: {\n    '$route.params.id': function (newId) {\n      this.cargarProductos(newId);\n      this.findCategory(newId);\n    }\n  },\n\n  created() {\n    this.cargarProductos(this.id);\n    this.findCategory(this.id);\n  },\n\n  mounted() {\n    window.scrollTo({\n      top: 0,\n      behavior: 'smooth'\n    });\n  }\n\n};","map":{"version":3,"sources":["C:\\Users\\JELOSKA ISABEL\\Documents\\wild-souls\\Wild-Souls\\src\\pages\\Categoria.vue"],"names":[],"mappings":"AAwCA,SAAS,GAAT,QAAoB,sBAApB;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,eADO;AAEb,EAAA,KAAK,EAAE,CAAC,IAAD,CAFM;;AAGb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,QAAQ,EAAE,EADL;AAEL,MAAA,YAAY,EAAE;AAFT,KAAP;AAID,GARY;;AASb,EAAA,OAAO,EAAE;AACP,IAAA,eAAe,CAAC,EAAD,EAAK;AAClB,MAAA,GAAG,CAAC,GAAJ,CAAQ,yBAAyB,EAAjC,EAAqC,IAArC,CAA2C,QAAD,IAAc;AACtD,aAAK,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,QAA9B;AACD,OAFD;AAGD,KALM;;AAMP,IAAA,YAAY,CAAC,EAAD,EAAK;AACf,MAAA,GAAG,CAAC,GAAJ,CAAQ,mBAAR,EAA6B,IAA7B,CAAmC,QAAD,IAAc;AAC9C,aAAK,IAAI,QAAT,IAAqB,QAAQ,CAAC,IAAT,CAAc,UAAnC,EAA+C;AAC7C,cAAI,QAAQ,CAAC,EAAT,IAAe,EAAnB,EAAuB;AACrB,iBAAK,YAAL,GAAoB,QAAQ,CAAC,aAA7B;AACF;AACF;AACD,OAND;AAOD;;AAdM,GATI;AAyBb,EAAA,KAAK,EAAE;AACL,wBAAoB,UAAS,KAAT,EAAgB;AAClC,WAAK,eAAL,CAAqB,KAArB;AACA,WAAK,YAAL,CAAkB,KAAlB;AACF;AAJK,GAzBM;;AA+Bb,EAAA,OAAO,GAAG;AACR,SAAK,eAAL,CAAqB,KAAK,EAA1B;AACA,SAAK,YAAL,CAAkB,KAAK,EAAvB;AACD,GAlCY;;AAmCb,EAAA,OAAO,GAAG;AACR,IAAA,MAAM,CAAC,QAAP,CAAgB;AAAE,MAAA,GAAG,EAAE,CAAP;AAAU,MAAA,QAAQ,EAAE;AAApB,KAAhB;AACD;;AArCY,CAAf","sourcesContent":["<template>\r\n  <div class=\"row\" style=\"background-color: #be5a4b\">\r\n    <div class=\"col-12 category-hero\">\r\n      <div class=\"row\">\r\n        <div class=\"col-sm-6 py-5 category-title\" style=\"background-color: #be5a4b\"></div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"row\" style=\"background-color: #be5a4b\">\r\n    <div class=\"col-12\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-sm-6 p-0\">\r\n            <h1 class=\"d-none d-sm-block\" style=\"font-size: 8vh\">{{ categoryName }}</h1>\r\n            <h2 class=\"d-block d-sm-none\">{{ categoryName }}</h2>\r\n          </div>\r\n        </div>\r\n        <div class=\"row offset-md-2\">\r\n          <div v-for=\"(product, index) in products\" :key=\"index\" class=\"col-sm-12 col-md-6\">\r\n            <img\r\n              :src=\"product.product_image\"\r\n              class=\"img-fluid product-card-img\"\r\n              style=\"width: 390px; height: 550px\"\r\n            />\r\n            <div class=\"mt-2 p-3\">\r\n              <h3>{{ product.product_name }}</h3>\r\n              <p>{{ product.product_price }} Bs.</p>\r\n              <button class=\"btn-product-card\" style=\"background-color: #000\">\r\n                <router-link :to=\"'/product/' + product.id\">Ver Producto</router-link>\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { api } from '@/config/site.config';\r\n\r\nexport default {\r\n  name: 'CategoriaPage',\r\n  props: ['id'],\r\n  data() {\r\n    return {\r\n      products: [],\r\n      categoryName: '',\r\n    };\r\n  },\r\n  methods: {\r\n    cargarProductos(id) {\r\n      api.get('categories/products/' + id).then((response) => {\r\n        this.products = response.data.products;\r\n      });\r\n    },\r\n    findCategory(id) {\r\n      api.get('categories/getAll').then((response) => {\r\n        for (let category of response.data.categories) {\r\n          if (category.id == id) {\r\n            this.categoryName = category.category_name;\r\n          }\r\n        }\r\n      });\r\n    },\r\n  },\r\n  watch: {\r\n    '$route.params.id': function(newId) {\r\n      this.cargarProductos(newId);\r\n      this.findCategory(newId);\r\n    }\r\n  },\r\n  created() {\r\n    this.cargarProductos(this.id);\r\n    this.findCategory(this.id);\r\n  },\r\n  mounted() {\r\n    window.scrollTo({ top: 0, behavior: 'smooth' });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}