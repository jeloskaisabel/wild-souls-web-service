{"ast":null,"code":"import { api } from '@/config/site.config';\nimport { storageSave, storageGet } from '@/services/storage.js';\nexport default {\n  name: 'GiftCardPage',\n\n  data() {\n    return {\n      productsCart: storageGet('wildBox') || [],\n      products: [],\n      searchText: '',\n      wildFriend: '',\n      dedication: '',\n      wildBoxOrder: '',\n      phone: '59174913903'\n    };\n  },\n\n  methods: {\n    cargarProductos() {\n      api.get('products/getAll').then(response => {\n        this.products = response.data.products;\n      });\n    },\n\n    searchProduct() {\n      if (this.searchText !== '') {\n        api.get('products/search', {\n          params: {\n            query: this.searchText\n          }\n        }).then(response => {\n          this.products = response.data.products;\n        });\n      } else {\n        this.cargarProductos();\n      }\n    },\n\n    addCart(id, product) {\n      let newProduct = {\n        id: product.id,\n        product_name: product.product_name,\n        product_price: product.product_price,\n        product_image: product.product_image,\n        quantity: 1\n      };\n      let currentProductsCart = storageGet('wildBox') || [];\n\n      if (!currentProductsCart.find(prod => prod.id === id)) {\n        let newProductsCart = [...currentProductsCart, newProduct];\n        this.productsCart = newProductsCart;\n        storageSave('wildBox', newProductsCart);\n      }\n    },\n\n    addAmount(id) {\n      let currentProductsCart = storageGet('wildBox');\n      currentProductsCart.forEach(currentProd => {\n        if (currentProd.id === id) {\n          currentProd.quantity += 1;\n        }\n      });\n      this.productsCart = [...currentProductsCart];\n      storageSave('wildBox', currentProductsCart);\n    },\n\n    decreaseAmount(id) {\n      let currentProductsCart = storageGet('wildBox');\n      currentProductsCart.forEach(currentProd => {\n        if (currentProd.id === id && currentProd.quantity > 0) {\n          currentProd.quantity -= 1;\n        }\n      });\n      this.productsCart = [...currentProductsCart];\n      storageSave('wildBox', currentProductsCart);\n    },\n\n    removeProduct(id) {\n      let currentProductsCart = storageGet('wildBox');\n      currentProductsCart = currentProductsCart.filter(prod => prod.id !== id);\n      this.productsCart = [...currentProductsCart];\n      storageSave('wildBox', currentProductsCart);\n    },\n\n    productsOrder() {\n      this.wildBoxOrder = this.productsCart.map(prod => `- ${prod.quantity} ${prod.product_name}`).join('\\n');\n    },\n\n    submitOrder() {\n      this.productsOrder();\n      let message = `Mi Wild Box debe contener:\\n${this.wildBoxOrder}\\nMi amigo wild es: ${this.wildFriend}\\nLa dedicatoria es: ${this.dedication}`;\n      let link = `https://api.whatsapp.com/send?phone=${this.phone}&text=${encodeURIComponent(message)}`;\n      window.location = link;\n      console.log(message);\n    }\n\n  },\n\n  created() {\n    this.cargarProductos();\n  },\n\n  mounted() {\n    window.scrollTo({\n      top: 0,\n      behavior: 'smooth'\n    });\n  }\n\n};","map":{"version":3,"sources":["C:\\Users\\JELOSKA ISABEL\\Documents\\wild-souls\\Wild-Souls\\src\\pages\\GiftCard.vue"],"names":[],"mappings":"AAyJA,SAAS,GAAT,QAAoB,sBAApB;AACA,SAAS,WAAT,EAAsB,UAAtB,QAAwC,uBAAxC;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,cADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,YAAY,EAAE,UAAU,CAAC,SAAD,CAAV,IAAyB,EADlC;AAEL,MAAA,QAAQ,EAAE,EAFL;AAGL,MAAA,UAAU,EAAE,EAHP;AAIL,MAAA,UAAU,EAAE,EAJP;AAKL,MAAA,UAAU,EAAE,EALP;AAML,MAAA,YAAY,EAAE,EANT;AAOL,MAAA,KAAK,EAAE;AAPF,KAAP;AASD,GAZY;;AAab,EAAA,OAAO,EAAE;AACP,IAAA,eAAe,GAAG;AAChB,MAAA,GAAG,CAAC,GAAJ,CAAQ,iBAAR,EAA2B,IAA3B,CAAiC,QAAD,IAAc;AAC5C,aAAK,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,QAA9B;AACD,OAFD;AAGD,KALM;;AAMP,IAAA,aAAa,GAAG;AACd,UAAI,KAAK,UAAL,KAAoB,EAAxB,EAA4B;AAC1B,QAAA,GAAG,CAAC,GAAJ,CAAQ,iBAAR,EAA2B;AAAE,UAAA,MAAM,EAAE;AAAE,YAAA,KAAK,EAAE,KAAK;AAAd;AAAV,SAA3B,EAAmE,IAAnE,CAAyE,QAAD,IA