{"ast":null,"code":"import json from '../recipes.json';\nimport { api } from '@/config/site.config';\nexport default {\n  name: 'RecetaPage',\n  props: ['id'],\n\n  data() {\n    return {\n      recipe: json.recipes[this.id - 1],\n      productsInUse: []\n    };\n  },\n\n  methods: {},\n  computed: {},\n  watch: {},\n\n  created() {},\n\n  mounted() {\n    window.scrollTo({\n      top: 0,\n      behavior: 'smooth'\n    });\n\n    for (let x in this.recipe.recipe_products_in_use) api.get('products/getProduct/' + this.recipe.recipe_products_in_use[x]).then(response => this.productsInUse.push(response.data.product));\n  } // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n\n\n};","map":{"version":3,"sources":["C:\\Users\\JELOSKA ISABEL\\Documents\\wild-souls\\Wild-Souls\\src\\pages\\Receta.vue"],"names":[],"mappings":"AA8EA,OAAO,IAAP,MAAiB,iBAAjB;AACA,SAAS,GAAT,QAAoB,sBAApB;AACA,eAAe;AACb,EAAA,IAAI,EAAE,YADO;AAEb,EAAA,KAAK,EAAE,CAAC,IAAD,CAFM;;AAGb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,MAAM,EAAE,IAAI,CAAC,OAAL,CAAa,KAAK,EAAL,GAAU,CAAvB,CADH;AAEL,MAAA,aAAa,EAAE;AAFV,KAAP;AAID,GARY;;AASb,EAAA,OAAO,EAAE,EATI;AAUb,EAAA,QAAQ,EAAE,EAVG;AAWb,EAAA,KAAK,EAAE,EAXM;;AAYb,EAAA,OAAO,GAAG,CAAE,CAZC;;AAab,EAAA,OAAO,GAAG;AACR,IAAA,MAAM,CAAC,QAAP,CAAgB;AAAC,MAAA,GAAG,EAAC,CAAL;AAAQ,MAAA,QAAQ,EAAE;AAAlB,KAAhB;;AACA,SAAK,IAAI,CAAT,IAAc,KAAK,MAAL,CAAY,sBAA1B,EACE,GAAE,CACC,GADH,CACO,yBAAyB,KAAK,MAAL,CAAY,sBAAZ,CAAmC,CAAnC,CADhC,EAEG,IAFH,CAES,QAAD,IAAc,KAAK,aAAL,CAAmB,IAAnB,CAAwB,QAAQ,CAAC,IAAT,CAAc,OAAtC,CAFtB;AAGH,GAnBY,CAqBb;AACA;AACA;;;AAvBa,CAAf","sourcesContent":["<template>\r\n  <div\r\n    class=\"row about-us-title d-flex justify-content-center align-items-center p-md-5\"\r\n    style=\"background-color: #fceeea\"\r\n  >\r\n    <div class=\"col-md-6 about-us-section\">\r\n      <h6 class=\"text-center mt-5 pb-md-4\">{{ recipe.recipe_category }}</h6>\r\n      <h1 class=\"text-center pb-md-4 d-none d-sm-block\" style=\"font-size: 8vh\">\r\n        {{ recipe.recipe_name }}\r\n      </h1>\r\n      <h2 class=\"text-center pb-md-4 d-block d-sm-none mb-5 mt-5\">{{ recipe.recipe_name }}</h2>\r\n      <h6 class=\"text-center\">Tiempo de preparación: {{ recipe.recipe_preparation_time }}</h6>\r\n      <h6 class=\"text-center\">Porciones: {{ recipe.recipe_portions }}</h6>\r\n      <h6 class=\"text-center\">Dificultad: {{ recipe.recipe_difficult }}</h6>\r\n    </div>\r\n  </div>\r\n  <div class=\"row\" style=\"background-color: #fceeea\">\r\n    <div\r\n      class=\"col-sm-12 col-md-5 d-flex align-items-center\"\r\n      style=\"background-color: #f5afb9; border-top-right-radius: 250px; height: 100vh\"\r\n    >\r\n      <div class=\"container\">\r\n        <div class=\"row d-flex px-md-5 mx-md-5 justify-content-center\">\r\n          <div class=\"col-sm-12 col-md-12\">\r\n            <h6>Ingredientes</h6>\r\n            <ul style=\"list-style: disc\">\r\n              <li\r\n                v-for=\"(ingredient, index) in recipe.recipe_ingredients\"\r\n                :key=\"index\"\r\n                class=\"ingredient\"\r\n              >\r\n                {{ ingredient }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-sm-12 col-md-7 d-flex align-items-center justify-content-center\">\r\n      <div class=\"container\">\r\n        <div class=\"col-sm-12 col-md-8 p-sm-2 p-md-5 m-md-5\">\r\n          <h2>Instrucciones</h2>\r\n          <ul style=\"list-style: disc\">\r\n            <li v-for=\"(instruction, index) in recipe.recipe_instructions\" :key=\"index\">\r\n              {{ instruction }}\r\n            </li>\r\n          </ul>\r\n          <h2>¡Disfruta!</h2>\r\n          <img :src=\"recipe.recipe_image\" class=\"img-fluid\" style=\"height: 60vh\" align=\"center\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"row py-5\" style=\"background-color: #fceeea\">\r\n    <div class=\"col-12\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <h1 class=\"pb-md-4 d-none d-sm-block\" style=\"font-size: 8vh\">\r\n        Productos Wild en uso\r\n      </h1>\r\n      <h2 class=\"text-center  d-block d-sm-none mb-5 mt-5\">Productos Wild en uso</h2>\r\n      <h6 class=\"pb-md-4\">Usa nuestros productos:</h6>\r\n          <div v-for=\"(product, index) in productsInUse\" :key=\"index\" class=\"col-sm-12 col-md-4\">\r\n            <img :src=\"product.product_image\" class=\"img-fluid product-card-img\" />\r\n            <div class=\"mt-2 p-3\">\r\n              <h3>{{ product.product_name }}</h3>\r\n              <p>{{ product.product_price }} Bs.</p>\r\n              <button class=\"btn-product-card\" style=\"background-color: #000\">\r\n                <router-link :to=\"'/product/' + product.id\">Ver Producto</router-link>\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport json from '../recipes.json';\r\nimport { api } from '@/config/site.config';\r\nexport default {\r\n  name: 'RecetaPage',\r\n  props: ['id'],\r\n  data() {\r\n    return {\r\n      recipe: json.recipes[this.id - 1],\r\n      productsInUse: [],\r\n    };\r\n  },\r\n  methods: {},\r\n  computed: {},\r\n  watch: {},\r\n  created() {},\r\n  mounted() {\r\n    window.scrollTo({top:0, behavior: 'smooth'});\r\n    for (let x in this.recipe.recipe_products_in_use)\r\n      api\r\n        .get('products/getProduct/' + this.recipe.recipe_products_in_use[x])\r\n        .then((response) => this.productsInUse.push(response.data.product));\r\n  },\r\n\r\n  // Se pueden utilizar estos hooks para el ciclo de vida\r\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\r\n  // activated, deactivated, beforeUnmount, unmounted\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}